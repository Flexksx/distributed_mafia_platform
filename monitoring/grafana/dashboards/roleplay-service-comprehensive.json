    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"roleplay-service\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "instance_id",
          "type": "query",
          "query": "label_values(service_discovery_registration_status{job=\"roleplay-service\"}, instance_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "route",
          "type": "query",
          "query": "label_values(http_requests_total{job=\"roleplay-service\"}, route)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "method",
