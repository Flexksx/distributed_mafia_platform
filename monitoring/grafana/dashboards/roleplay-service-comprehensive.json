{
  "dashboard": {
    "id": null,
    "title": "Roleplay Service - Comprehensive Monitoring",
    "tags": [
      "mafia",
      "roleplay-service",
      "comprehensive"
    ],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"roleplay-service\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Service Discovery Registration",
        "type": "stat",
        "targets": [
          {
            "expr": "service_discovery_registration_status{job=\"roleplay-service\"}",
            "legendFormat": "{{instance_id}} - Registered"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Service Health Status",
